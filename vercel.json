{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/((?!api|_next|static|assets|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
